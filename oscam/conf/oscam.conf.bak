#
# main configuration
#

[global]
nice	      = -1
#WaitForCards  = 1
ClientTimeout   = 5
LogFile     	= stdout
#Level		= 15
CacheDelay  	= 120

# logging

#pidfile	      = /var/run/oscam.pid
logfile	      = /var/log/oscam/oscam.log
usrfile	      = /var/log/oscam/oscamuser.log
cwlogdir      = /var/log/oscam/cw

# monitor

# protocols

[newcamd]
key           = 0102030405060708091011121314
#port          = 10000@0100:FFFFFF;10001@0200:FFF000,FFFF00;10002@0300:FFFFFF
port         	= 4000@0500:023700,030600

[webif]
httpport      = 8080
httpuser      = myoscam
httppwd       = jcrfv
httpallowed   = 127.0.0.1,192.168.1.22,192.168.27.5,10.10.10.1